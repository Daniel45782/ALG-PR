import math
x = int(input("Введите число"))
A = 1 - math.cos(x) / 2 * 0.5
print(A)
#333333333333333333333333333333
from math import sin , cos , pow , log , exp
x = int(input("Введите число"))
A = 0.5 * abs(x) * log(x**2) / -5.0 / 4 * x + exp(x/2)
print (A)





#444444444444444444444444444444
from math import sin , cos , pow
x = int(input("Введите число"))
y = int(input("Введите число"))
a = pow(sin(x),2) + pow(cos( pow(y,3)),2)* 0.5
print(a)
